@model gacha.Models.gachaMachine
@if (Model.machinePictureName == null)
{
    <img src="~/img/no-photo-available.png" title="無圖案" style="width:320px;height:240px"  />
}
else
{
    <img src="~/img/@Html.DisplayFor(model => model.machinePictureName)" title="@Html.DisplayFor(model => model.machinePictureName)" style="width:320px;height:240px" />

}