@model gacha.Models.gachaTheme

@{
    ViewData["Title"] = "Create";
}
@section Styles {
    <link href="~/css/datatables.css" rel="stylesheet" />
}


<div class="row">
    <div class="col-md-4">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="themeName" class="control-label"></label>
            <input asp-for="themeName" class="form-control" />
            <span asp-validation-for="themeName" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="新增" class="btn button" />
        </div>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn button"><i class="fa-solid fa-backward"></i></a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
