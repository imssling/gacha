@model gacha.Models.trackingList

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>trackingList</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="userId" class="control-label"></label>
                <select asp-for="userId" class ="form-control" asp-items="ViewBag.userId"></select>
            </div>
            <div class="form-group">
                <label asp-for="gachaMachineId" class="control-label"></label>
                <select asp-for="gachaMachineId" class ="form-control" asp-items="ViewBag.gachaMachineId"></select>
            </div>
            <div class="form-group">
                <label asp-for="noteStatus" class="control-label"></label>
                <input asp-for="noteStatus" class="form-control" placeholder="通知狀態:待通知,無須通知,已通知" />
                <span asp-validation-for="noteStatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
